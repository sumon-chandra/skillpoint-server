		"@types/bcryptjs": "^3.0.0",
		"@types/cookie-parser": "^1.4.10",
		"@types/cors": "^2.8.19",
		"@types/express-session": "^1.18.2",
		"@types/multer": "^2.0.0",
		"bcryptjs": "^3.0.3",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"dotenv": "^17.2.3",
		"eslint": "^9.39.2",
		"express": "^4.18.2",
		"express-session": "^1.18.2",
		"http-status-codes": "^2.3.0",
